{
  "name": "weather-bot-for-discord",
  "version": "0.1.0",
  "private": true,
  "description": "This get weather forecast from Dark Sky API, and post it to Discord. It works with Google App Script.",
  "scripts": {
    "lint": "eslint --fix src/**/*.ts",
    "test": "jest",
    "build": "npm run lint && npm run test && rimraf dist && webpack && cpx src/appsscript.json dist",
    "deploy": "npm run build && clasp push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proudust/weather-bot-for-discord.git"
  },
  "author": "proudust",
  "license": "MIT",
  "devDependencies": {
    "@types/google-apps-script": "0.0.53",
    "@types/jest": "24.0.15",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "clasp": "1.0.0",
    "cpx": "1.5.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-googleappsscript": "1.0.2",
    "eslint-plugin-jest": "22.7.1",
    "eslint-plugin-prettier": "3.1.0",
    "gas-webpack-plugin": "1.0.2",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "typescript": "3.5.2",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.4"
  },
  "jest": {
    "testRegex": "(src/.+(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
