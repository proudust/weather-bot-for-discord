{
  "name": "weather-bot-for-discord",
  "version": "0.1.0",
  "private": true,
  "description": "This get weather forecast from Dark Sky API, and post it to Discord. It works with Google App Script.",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts --fix .",
    "test": "jest",
    "build": "npm run lint && npm run test && rimraf dist && webpack && cpy src/appsscript.json dist",
    "deploy": "npm run build && clasp push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proudust/weather-bot-for-discord.git"
  },
  "author": "proudust",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "2.3.0",
    "@types/google-apps-script": "1.0.10",
    "@types/jest": "24.9.1",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "cpy-cli": "^3.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-googleappsscript": "1.0.3",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-prettier": "3.1.2",
    "gas-webpack-plugin": "1.0.2",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "ts-jest": "25.0.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
