{
  "name": "weather-bot-for-discord",
  "version": "0.1.0",
  "private": true,
  "description": "This get weather forecast from Dark Sky API, and post it to Discord. It works with Google App Script.",
  "scripts": {
    "lint": "eslint --fix src/**/*.ts",
    "test": "jest",
    "build": "npm run lint && npm run test && rimraf dist && webpack && cpx src/appsscript.json dist",
    "deploy": "npm run build && clasp push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proudust/weather-bot-for-discord.git"
  },
  "author": "proudust",
  "license": "MIT",
  "devDependencies": {
    "@types/google-apps-script": "0.0.37",
    "@types/jest": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "clasp": "^1.0.0",
    "cpx": "^1.5.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-googleappsscript": "^1.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "gas-webpack-plugin": "^1.0.2",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "jest": {
    "testRegex": "(src/.+(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
